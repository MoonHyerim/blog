<h1>Listing hyes</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @hyes.each do |hye| %>
  <tr>
    <td><%= hye.name %></td>
    <td><%= link_to 'Show', hye %></td>
    <td><%= link_to 'Edit', edit_hye_path(hye) %></td>
    <td><%= link_to 'Destroy', hye, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Hye', new_hye_path %>
